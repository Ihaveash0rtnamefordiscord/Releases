local Runservice = game:GetService("RunService")
local femboy = gcinfo()
local lol = 0

function CreateChatMessage(Msg) -- Create Function With Msg Variable
	game.StarterGui:SetCore("ChatMakeSystemMessage",{Text = Msg,Color = Color3.fromRGB(0, 255, 127),Font = Enum.Font.Highway})
end

pcall(function()
Runservice.Stepped:Connect(function()

		if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed > 16 then
			CreateChatMessage("" .. game.Players.LocalPlayer.Name .. " Was Caught by Bork Anti-Cheat (Spoofing Speed)")
			game:GetService("TeleportService"):Teleport(game.PlaceId)
			wait(1)
	end
		if game.Players.LocalPlayer.Character.Humanoid.JumpPower > 50 then
			CreateChatMessage("" .. game.Players.LocalPlayer.Name .. " Was Caught by Bork Anti-Cheat (Spoofing JumpPower)")

			game:GetService("TeleportService"):Teleport(game.PlaceId)
			wait(1)

	end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.ClassName == "LocalScript" then
				if v.Disabled == true then
					CreateChatMessage("" .. game.Players.LocalPlayer.Name .. " Was Caught by Bork Anti-Cheat (Modifing Local Scripts!)")
					game:GetService("TeleportService"):Teleport(game.PlaceId)
					wait(1)

				end
			end
		end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.ClassName == "LocalScript" then
				if v then
					wait()
				else
					CreateChatMessage("" .. game.Players.LocalPlayer.Name .. " Was Caught by Bork Anti-Cheat (Modifing Local Scripts!)")
					game:GetService("TeleportService"):Teleport(game.PlaceId)
					wait(1)

				end
			end
		end	
		if gcinfo()-femboy>6000 then
			CreateChatMessage("" .. game.Players.LocalPlayer.Name .. " Was Caught by Bork Anti-Cheat (Execution of Illegal Gui)")
			game:GetService("TeleportService"):Teleport(game.PlaceId)
			wait(1)

	end
		femboy = gcinfo()
		while wait() do
			local num=gcinfo(math.randomseed(1))
			if num==294 then
				lol+=1
			else
				lol=0
			end
			if lol>25 then
				game:GetService("TeleportService"):Teleport(game.PlaceId)
			end
			print(lol)--idk
		end
end)
end)



